{
  "name": "emcd",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "linter": "npx eslint",
    "docs": "jsdoc -c jsdoc.json",
    "open-docs": "open ./docs/index.html",
    "docs-pedantic": "jsdoc -c jsdoc.json --pedantic ",
    "docs:watch": "nodemon --watch src -e js,vue --exec 'npm run docs'"
  },
  "dependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "better-docs": "^2.7.3",
    "clean-jsdoc-theme": "^4.3.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jsdoc": "^4.0.4",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "tsd-jsdoc": "^2.5.0",
    "vite": "^5.4.10",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.1.10"
  }
}
